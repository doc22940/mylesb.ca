<h1>{{ site.title }}</h1>
<h2>{{ site.description }}</h2>

<ul class="contact">
{{#each site.contact}}
    <li class="{{ slug }}">
        <a href="{{ url }}" title="{{ description }}">
            {{ title }}
        </a>
    </li>
{{/each}}
</ul>

<ul class="elsewhere">
{{#each site.elsewhere}}
    <li class="{{ slug }}">
        <a href="{{ url }}" title="{{ description }}">
            {{ title }}
        </a>
    </li>
{{/each}}
</ul>
