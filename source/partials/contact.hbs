<div class="panel panel-contact background-{{contact.background}}" id="contact">
	<div class="container">
		<div class="content">
			<h1>Contact Me</h1>
			
			<hr>
			
			<form action="index_submit" method="get" accept-charset="utf-8">
				<p>
					<label for="id_name">Name</label>
					<input type="text" id="id_name" name="name">
				</p>
				
				<p>
					<label for="id_email">Email</label>
					<input type="email" id="id_email" name="email">
				</p>
				
				<p>
					<label for="id_message">Message</label>
					<textarea type="message" id="id_message" name="message"></textarea>
				</p>
				
				<p>
					<label for="id_indian_jones">How many Indiana Jones films are there?</label>
					<select name="indian_jones" id="id_indian_jones">
						<option value="4">Four</option>
						<option value="3">Three</option>
					</select>
				</p>
				
				<p><input type="submit" value="Send"></p>
			</form>
		</div>
	</div>
</div>