<!DOCTYPE html>
<html class="no-js" lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">

        <title>{{#if page.title}}{{ page.title }}{{else}}{{ site.title }}{{/if}}</title>

        <meta name="description" content="{{ site.description }}">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link rel="stylesheet" href="/assets/css/style.css">

        {{> favicons }}

        <link rel="canonical" href="https://mylesb.ca{{canonical page.dest}}">
        <link rel="onion" href="http://jx5z7chkv7wuvobn.onion{{canonical page.dest}}">
    </head>
    <body class="body-{{basename}}">
        <div class="page page-{{basename}}">
            {{> body}}
        </div>

        {{> analytics }}
        <script src="/assets/javascript/jquery-2.2.0.min.js"></script>
        <script src="/assets/javascript/script.js"></script>
    </body>
</html>
