<h2 class="c-slide__title">Elsewhere</h2>

<div class="c-slide__row">
  <div class="c-slide__col-6">
    <div class="c-list-group c-list-group--elsewhere">
      {{#each elsewhere.web}}
      <a class="c-list-group__item c-list-group__item--{{this.slug}} u-url" href="{{this.url}}"{{#if this.rel}} rel="{{this.rel}}"{{/if}} alt="{{this.name}}">
        <div class="c-list-group__header">
          <h3 class="c-list-group__title">{{this.name}}</h3>
          <small class="c-list-group__url">{{#if this.display_url}}{{this.display_url}}{{else}}{{this.url}}{{/if}}</small>
        </div>
      </a>
      {{/each}}
    </div>
  </div>

  <div class="c-slide__col-6">
    <div class="c-list-group c-list-group--elsewhere">
      {{#each elsewhere.social}}
      <a class="c-list-group__item c-list-group__item--{{this.slug}} u-url" href="{{this.url}}"{{#if this.rel}} rel="{{this.rel}}"{{/if}}>
        <h3 class="c-list-group__title">{{this.name}}</h3>
      </a>
      {{/each}}
    </div>
  </div>
</div>
